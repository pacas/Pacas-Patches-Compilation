                    <patchName>Research_Tables_HitechLabStation</patchName>
                    <arguments>
                        <li>AM_UltraHeavyMechtech</li>
                    </arguments>
                </li>
                <!--New Techs-->
                <li Class="XmlExtensions.ApplyPatch">
                    <patchName>Research_Positions_Cost_Techlevel</patchName>
                    <arguments>
                        <li>AM_MechanoidBeamcasting</li>
                        <li>500</li>
                        <li>22</li>
                        <li>74</li>
                        <li>Ultra</li>
                    </arguments>
