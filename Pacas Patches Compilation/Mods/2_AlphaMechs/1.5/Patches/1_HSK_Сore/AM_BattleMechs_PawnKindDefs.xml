<?xml version="1.0" encoding="utf-8"?>
<Patch>
    <Operation Class="PatchOperationSequence">
        <operations>
            <!--PawnKindDef-->
            <li Class="PatchOperationAttributeSet">
                <xpath>Defs/PawnKindDef[
                defName="AM_Aura" or 
                defName="AM_Daggersnout" or 
                defName="AM_Demolisher" or 
                defName="AM_Fireworm" or 
                defName="AM_Goliath" or 
                defName="AM_Phalanx" or 
                defName="AM_Siegebreaker"]</xpath>
                <attribute>ParentName</attribute>
                <value>SK_BaseMechanoidKind</value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Aura"]/combatPower</xpath>
                <value>
                    <combatPower>450</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Daggersnout"]/combatPower</xpath>
                <value>
                    <combatPower>250</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Fireworm"]/combatPower</xpath>
                <value>
                    <combatPower>500</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Demolisher"]/combatPower</xpath>
                <value>
                    <combatPower>850</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Goliath"]/combatPower</xpath>
                <value>
                    <combatPower>950</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Phalanx"]/combatPower</xpath>
                <value>
                    <combatPower>550</combatPower>
                </value>
            </li>
            <li Class="PatchOperationReplace">
                <xpath>Defs/PawnKindDef[defName="AM_Siegebreaker"]/combatPower</xpath>
                <value>
                    <combatPower>900</combatPower>
                </value>
            </li>
        </operations>
    </Operation>
</Patch>