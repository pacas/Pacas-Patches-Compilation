                    <patchName>Research_Positions_Tab</patchName>
                    <arguments>
                        <li>HighMechtech</li>
                        <li>19</li>
                        <li>72</li>
                        <li>AM_AlphaMechs</li>
                    </arguments>
                </li>
                <li Class="XmlExtensions.ApplyPatch">
                    <patchName>Research_Positions_Tab</patchName>
                    <arguments>
                        <li>UltraMechtech</li>
                        <li>21</li>
                        <li>72</li>
                        <li>AM_AlphaMechs</li>
                    </arguments>
                </li>
                <li Class="XmlExtensions.ApplyPatch">
                    <patchName>Research_Positions_Tab</patchName>
                    <arguments>
                        <li>WastepackAtomizer</li>
                        <li>15</li>
                        <li>73</li>
                        <li>AM_AlphaMechs</li>
                    </arguments>
                </li>
                <!--New Mechs-->
                <li Class="SK.PatchOperationReplaceExtended">
                    <xpath>Defs/ResearchProjectDef[defName="AM_WorkerStandardMechtech"]</xpath>
                    <value>
                        <baseCost>250</baseCost>
                        <researchViewX>13</researchViewX>
                        <researchViewY>73</researchViewY>
                        <techLevel>Industrial</techLevel>
                    </value>
                </li>
                <li Class="SK.PatchOperationReplaceExtended">
                    <xpath>Defs/ResearchProjectDef[defName="AM_StandardMechtech"]</xpath>
                    <value>
                        <baseCost>1000</baseCost>
                        <researchViewX>16</researchViewX>
                        <researchViewY>73</researchViewY>
                        <techLevel>Industrial</techLevel>
                    </value>
                </li>
                <li Class="SK.PatchOperationReplaceExtended">
                    <xpath>Defs/ResearchProjectDef[defName="AM_HeavyMechtech"]</xpath>
                    <value>
                        <baseCost>2000</baseCost>
                        <researchViewX>20</researchViewX>
                        <researchViewY>73</researchViewY>
