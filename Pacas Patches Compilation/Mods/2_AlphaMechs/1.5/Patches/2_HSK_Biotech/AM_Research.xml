<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Biotech</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationFindMod">
                    <mods>
                        <li>Ideology</li>
                    </mods>
                    <match Class="PatchOperationReplace">
                        <xpath>Defs/ResearchProjectDef[defName="NeuralSupercharger"]/researchViewY</xpath>
                        <value>
                            <researchViewY>68.00</researchViewY>
                        </value>
                    </match>
                </li>
                <!--<li Class="PatchOperationRemove">
                    <xpath>Defs/ResearchProjectDef[@Name="AM_MechtechBase"]/techLevel</xpath>
                </li>-->
                <!--Vanilla-->
                <li Class="XmlExtensions.ApplyPatch">
                    <patchName>Research_Positions_Tab</patchName>
                    <arguments>
                        <li>BasicMechtech</li>
                        <li>12</li>
                        <li>72</li>
                        <li>AM_AlphaMechs</li>
                    </arguments>
                </li>
                <li Class="XmlExtensions.ApplyPatch">
                    <patchName>Research_Positions_Tab</patchName>
                    <arguments>
                        <li>StandardMechtech</li>
                        <li>15</li>
                        <li>72</li>
                        <li>AM_AlphaMechs</li>
                    </arguments>
                </li>
