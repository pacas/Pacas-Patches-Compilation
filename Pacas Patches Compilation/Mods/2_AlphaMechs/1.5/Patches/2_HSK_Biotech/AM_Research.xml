                    <patchName>Research_Tables_HitechLabStation</patchName>
                    <arguments>
                        <li>AM_UltraHeavyMechtech</li>
                    </arguments>
                </li>
                <!--New Techs-->
                <li Class="SK.PatchOperationReplaceExtended">
                    <xpath>Defs/ResearchProjectDef[defName="AM_MechanoidBeamcasting"]</xpath>
