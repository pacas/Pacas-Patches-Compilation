<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>HSK more content</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
			
				<li Class="PatchOperationReplace">
					<xpath>/Defs/PawnKindDef[@ParentName="HMCNorbalBase" and defName="HMCCyberNorbal"]/techHediffsRequired/li[text()="GTHand"]</xpath>
					<value>
						<li>ArchotechHand</li>
					</value>
				</li>
				
				<li Class="PatchOperationReplace">
					<xpath>/Defs/PawnKindDef[@ParentName="HMCNorbalBase" and defName="HMCCyberNorbal"]/techHediffsRequired/li[text()="GTFoot"]</xpath>
					<value>
						<li>ArchotechFoot</li>
					</value>
				</li>
				
			</operations>
		</match>
	</Operation>

</Patch>