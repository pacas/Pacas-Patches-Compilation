		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationReplace">
					<xpath>Defs/ThingDef[defName="ANN_EnergyFist"]/costList/GTHand</xpath>
					<value>
						<ArchotechHand>1</ArchotechHand>
					</value>
				</li>

				<li Class="PatchOperationRemove">
					<xpath>/Defs/ThingDef[@ParentName="CombatAdvancedBionicsBase_Module" and defName="ANN_OuterBlade"]/costList</xpath>
				</li>
				<li Class="PatchOperationAddCopy">
					<xpath>/Defs/ThingDef[@ParentName="CombatAdvancedBionicsBase_Module" and defName="ANN_OuterBlade"]</xpath>
