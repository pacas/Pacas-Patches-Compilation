<?xml version="1.0" encoding="utf-8" ?>
<Patch>
    <Operation Class="PatchOperationSequence">
        <operations>
            <!--Arm Groups-->
            <li Class="PatchOperationAdd">
                <xpath>Defs/BodyDef[defName="Skaven" or defName="SkavenHorned" or defName="RatOgre"]/corePart/parts/li[def="Shoulder"]/parts/li[customLabel="left arm"]/groups</xpath>
                <value>
                    <li>LeftArm</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/BodyDef[defName="Skaven" or defName="SkavenHorned" or defName="RatOgre"]/corePart/parts/li[def="Shoulder"]/parts/li[customLabel="right arm"]/groups</xpath>
                <value>
                    <li>RightArm</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/BodyDef[defName="Skaven" or defName="SkavenHorned" or defName="RatOgre"]/corePart/parts/li[customLabel="left shoulder"]/groups</xpath>
                <value>
                    <li>LeftShoulder</li>
                </value>
            </li>
            <li Class="PatchOperationAdd">
                <xpath>Defs/BodyDef[defName="Skaven" or defName="SkavenHorned" or defName="RatOgre"]/corePart/parts/li[customLabel="right shoulder"]/groups</xpath>
                <value>
                    <li>RightShoulder</li>
                </value>
            </li>
        </operations>
    </Operation>
</Patch>