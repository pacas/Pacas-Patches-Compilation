<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="XmlExtensions.FindMod">
		<mods>
			<li>Free Will</li>
		</mods>
		<caseTrue>
            <Operation Class="PatchOperationAdd">
                <xpath>Defs/ThingDef[@Name="SK_BasePawn"]/inspectorTabs</xpath>
                <value>
                    <li>FreeWill.ITab_Pawn_FreeWill</li>
                </value>
            </Operation>
		</caseTrue>
	</Operation>
</Patch>