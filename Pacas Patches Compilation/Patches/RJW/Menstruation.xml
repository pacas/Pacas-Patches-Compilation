<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>RJW Menstruation Cycle</li>
        </mods>
        <match Class="PatchOperationSequence">
            <operations>
                <li Class="PatchOperationAdd">
                    <xpath>Defs/ThingDef[defName="OvaryRegenerationPill" or
                                          defName="SuperovulationInducingAgent" or
                                          defName="PainReliever" or
                                          defName="Cyclosporine"]
                    </xpath>
                    <value>
                        <thingCategories Inherit="False">
                            <li>RJWDrugs</li>
                        </thingCategories>
                    </value>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>